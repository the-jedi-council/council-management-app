{
  "name": "council-management-app",
  "version": "0.0.1",
  "description": "App to manage the Jedi Council members directory",
  "main": "api/app.js",
  "repository": "git@github.com:the-jedi-council/council-management-app.git",
  "author": "The Jedi Council",
  "scripts": {
    "start": "npm-run-all --parallel watch:server start:web",
    "start:web": "cd ui && react-scripts start",
    "start:server": "node api/server",
    "watch:server": "nodemon --watch api/server api/server",
    "build": "cd ui && react-scripts build",
    "test": "cd ui && react-scripts test --env=jsdom",
    "eject": "cd ui && react-scripts eject"
  },
  "dependencies": {
    "@material-ui/core": "4.5.1",
    "@material-ui/icons": "4.5.1",
    "@okta/jwt-verifier": "2.1.0",
    "@okta/okta-react": "3.0.8",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "create-react-app": "^4.0.3",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "finale-rest": "1.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "5.1.2",
    "react-scripts": "4.0.3",
    "sequelize": "6.6.2",
    "sqlite3": "5.0.2",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5"
  }
}
